<Window x:Class="NLog.Targets.InfluxDB.Demo.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:NLog.Targets.InfluxDB.Demo"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid>
            <Button Content="Info" HorizontalAlignment="Left" Margin="41,10,0,0" VerticalAlignment="Top" Height="75" Width="94" Click="Info_Click"/>
            <Button Content="Debug" HorizontalAlignment="Left" Margin="41,90,0,0" VerticalAlignment="Top" Height="75" Width="94" Click="Debug_Click"/>
            <Button Content="Warn" HorizontalAlignment="Left" Margin="41,170,0,0" VerticalAlignment="Top" Height="75" Width="94" Click="Warn_Click"/>
            <Button Content="Error" HorizontalAlignment="Left" Margin="41,250,0,0" VerticalAlignment="Top" Height="75" Width="94" Click="Error_Click"/>
        </Grid>

    </Grid>
</Window>
